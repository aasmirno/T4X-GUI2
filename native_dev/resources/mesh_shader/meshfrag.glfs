// mesh fragment shader
#version 420 core

uniform sampler2D image_texture;
uniform sampler2D data_texture;

in float H;
in vec2 TexCoord;

out vec4 frag_color;

void main()
{
    float h = H /10;
    frag_color = texture(data_texture, TexCoord) + texture(image_texture, TexCoord);
    //frag_color = vec4(h, h, h, 1.0);
}